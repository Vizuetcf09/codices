---
interface Props {
  title: string;
}

const { title } = Astro.props;
// components
import BaseHead from "@/components/BaseHead.astro";
import Header from "@/components/Header.astro";
---

<!doctype html>
<html lang="es">
  <BaseHead title={title} />
  <body class="body">
    <Header />
    <main>
      <slot />
    </main>
  </body>
</html>
